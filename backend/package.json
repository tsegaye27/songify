{
  "name": "songify",
  "version": "1.0.0",
  "description": "Professional music playlist management API built with TypeScript, Express, and MongoDB",
  "packageManager": "pnpm@10.9.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "nodemon -r tsconfig-paths/register dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --project tsconfig.json src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "seed": "ts-node -r tsconfig-paths/register --project tsconfig.json src/scripts/seedData.ts"
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^3.0.2",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.2.0",
    "http-status": "^1.8.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.4",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "typescript",
    "express",
    "mongodb",
    "music",
    "playlist",
    "authentication",
    "api"
  ],
  "author": "Tsegaye Talegngeta",
  "license": "MIT"
}
